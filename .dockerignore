# Dependencies (se instalan en el build)
/vendor
/node_modules

# Env local (Docker usa variables de docker-compose)
.env
.env.*
!.env.example

# Git y IDE
.git
.gitignore
.idea
.vscode
*.swp
*.swo

# Logs y cache (se recrean en el contenedor)
/storage/logs/*
/storage/framework/cache/*
/storage/framework/sessions/*
/storage/framework/views/*
/bootstrap/cache/*
!/storage/logs/.gitkeep
!/storage/framework/cache/.gitkeep
!/storage/framework/sessions/.gitkeep
!/storage/framework/views/.gitkeep
!/bootstrap/cache/.gitkeep

# Tests y docs (opcional, no necesarios para ejecutar)
/tests
.phpunit.result.cache
phpunit.xml
*.md
